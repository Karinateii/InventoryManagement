@model List<LabSupply>

<div class="container">
    <div class="row pt-4 pb-3">
        <div class="col-6">
            <h2 class="text-primary">
                Lab Supply 
            </h2>
        </div>
        <div class="col-6 text-end">
            <a asp-asp-controller="LabSupplies" asp-action="Upsert" class="btn btn-primary">
                <i class="bi bi-plus-circle"></i> Create New LabSupply
            </a>
        </div>
    </div>

    <table id="tblData" class="table table-bordered table-striped" style="width:100%">
        <thead>
            <tr>
                <th>
                    Supply Name
                 </th>
                <th>
                     Quantity On Hand
                </th>
                <th>
                    ReOrder Point
                </th>
                <th>
                    Supplier
                </th>
            <th></th>
            </tr>
        </thead>
    @*<tbody>

    </tbody>*@
</table>
</div>

@section Scripts{
    <script src="~/js/supply.js"></script>
}
